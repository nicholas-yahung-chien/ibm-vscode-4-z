{
    "extensions.autoCheckUpdates": false,
    "extensions.autoUpdate": false,
    "update.enableWindowsBackgroundUpdates": false,
    "update.mode": "none",
    "java.configuration.runtimes": [
        "_JAVA_RUNTIMES_"
    ],
    "java.home": "_JAVAHOME_",
    "java.jdt.ls.java.home": "_JAVAHOME_",
    "zopeneditor.JAVA_HOME": "_JAVAHOME_",
    "files.associations": {
        "**/*.COBOL*{,/*}": "cobol",
        "**/*.COB*{,/*}": "cobol",
        "**/*.COBCOPY*{,/*}": "cobol",
        "**/*.COPYBOOK*{,/*}": "cobol",
        "**/*.COPY*{,/*}": "cobol",
        "**/*.PL1*{,/*}": "pl1",
        "**/*.PLI*{,/*}": "pl1",
        "**/*.INC*{,/*}": "pl1",
        "**/*.INCLUDE*{,/*}": "pl1",
        "**/*.JCL*{,/*}": "jcl",
        "**/*.CNTL*{,/*}": "jcl",
        "**/*.PROC*{,/*}": "jcl",
        "**/*.ASM*{,/*}": "hlasm",
        "**/*.ASSEMBLE*{,/*}": "hlasm",
        "**/*.HLASM*{,/*}": "hlasm",
        "**/*.HLA*{,/*}": "hlasm",
        "**/*.MACLIB*{,/*}": "hlasm",
        "**/*.DBD*{,/*}": "hlasm",
        "**/*.MFS*{,/*}": "hlasm",
        "**/*.PCB*{,/*}": "hlasm",
        "**/*.EXEC*{,/*}": "rexx",
        "**/*.REXX*{,/*}": "rexx",
        "**/*.REXXINC*{,/*}": "rexx",
        "*.rex": "rexx",
        "**/.hlasmplugin/proc_grps.json": "jsonc",
        "**/.hlasmplugin/pgm_conf.json": "jsonc"
    },
    "zowe.cics.persistent": {
        "persistence": true,
        "programSearchHistory": [],
        "transactionSearchHistory": [],
        "localFileSearchHistory": [],
        "loadedCICSProfile": [],
        "librarySearchHistory": [],
        "datasetSearchHistory": [],
        "regionSearchHistory": [],
        "tcpipsSearchHistory": [],
        "urimapsSearchHistory": [],
        "pipelineSearchHistory": [],
        "webserviceSearchHistory": []
    },
    "cobol-lsp.cpy-manager.copybook-extensions": [
        ".CPY",
        ".COPY",
        ".cpy",
        ".copy",
        ""
    ],
    "cobol-lsp.cpy-manager.paths-local": [
        "_WORKSPACE_\\workspace\\*\\COPYBOOK",
        "_WORKSPACE_\\workspace\\*\\COPYLIB",
        "_WORKSPACE_\\workspace\\*\\COBCOPY",
        "_WORKSPACE_\\workspace\\*\\COPY"
    ],
    "redhat.telemetry.enabled": false,
    "python.venvPath": "_PYTHON_VENV_HOME_",
    "python.defaultInterpreterPath": "_PYTHON_VENV_EXEC_",
    "ansible.python.interpreterPath": "_PYTHON_VENV_EXEC_",
    "ansible.validation.lint.enabled": false,
    "yaml.schemas": {
        "_ZAPP_SCHEMA_URI_": [
            "zapp.yaml"
        ],
        "_ZCODE_FORMAT_SCHEMA_URI_": [
            "zcodeformat.yaml"
        ]
    },
    "json.schemas": [
        {
            "fileMatch": [
                "/zapp.json"
            ],
            "url": "_ZAPP_SCHEMA_URI_"
        },
        {
            "fileMatch": [
                "/zcodeformat.json"
            ],
            "url": "_ZCODE_FORMAT_SCHEMA_URI_"
        }
    ],
}